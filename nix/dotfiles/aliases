#!/bin/bash

# vim
alias vi="$(which vim)"
alias vim=$(which nvim)
alias v=$(which nvim)
alias vt="$(which nvim) +terminal"

# kubernetes
alias k8s="$HOME/Code/k8s.io/kubernetes"
